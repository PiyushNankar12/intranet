wb = xlsx_package.workbook

wb.add_worksheet(name: "Employees") do |sheet|
  sheet.add_row %w(Sr.No Employee_id  Name Email Role Designation status MobileNo Date_of_birth Date_of_Joining Date_of_relieving)
  counter = 0
  @usersxls.each do |user|
    counter += 1
    sheet.add_row [counter, user.employee_id, user.name, user.email, user.role, user.designation.name, user.status, user.mobile_number, user.date_of_birth,  user.date_of_joining, user.date_of_relieving]
  end
end
